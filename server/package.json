{
  "name": "ml-insights-hub-server",
  "version": "1.0.0",
  "description": "ML Insights Hub Backend",
  "main": "server.js",
  "scripts": {
    "start": "node server.js",
    "dev": "nodemon server.js",
    "test": "jest",
    "python-setup": "source ../venv/bin/activate && pip install -r requirements.txt",
    "python-activate": "source ../venv/bin/activate",
    "generate-jwt-secret": "node scripts/generate-jwt-secret.js",
    "setup-production-env": "node scripts/generate-jwt-secret.js --update-env",
    "check-security": "node scripts/generate-jwt-secret.js --check-env",
    "security-audit": "node scripts/security-audit.js",
    "security-full": "npm audit && node scripts/security-audit.js",
    "security:test-python": "node ../test-secure-python-bridge.js",
    "security:validate": "node utils/startupSecurity.js",
    "db:test": "node scripts/test-database-connection.js",
    "db:health": "node scripts/database-health-check.js",
    "db:stats": "node scripts/database-stats.js",
    "preproduction": "npm run security-audit && npm run generate-jwt-secret --update-env"
  },
  "keywords": [
    "ml",
    "mern",
    "real-estate"
  ],
  "author": "",
  "license": "ISC",
  "dependencies": {
    "bcryptjs": "^2.4.3",
    "bull": "^4.10.0",
    "compression": "^1.7.4",
    "cors": "^2.8.5",
    "dotenv": "^16.0.3",
    "express": "^4.18.2",
    "express-mongo-sanitize": "^2.2.0",
    "express-rate-limit": "^6.11.2",
    "express-validator": "^7.2.1",
    "helmet": "^7.0.0",
    "joi": "^17.9.0",
    "jsonwebtoken": "^9.0.0",
    "mongoose": "^7.0.0",
    "morgan": "^1.10.0",
    "multer": "^1.4.5-lts.1",
    "redis": "^4.6.0",
    "socket.io": "^4.6.0"
  },
  "devDependencies": {
    "concurrently": "^8.0.0",
    "nodemon": "^3.0.0"
  }
}
